<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="EZTutor.Data.Remote" namespace="EZTutor.Data.Remote.Entities" >
  <class name="Topics" table="Topics" lazy="true">
    <id name="Topicid" column="TopicID" type="Guid">
      <generator class="assigned" />
    </id>
    <property type="string" not-null="true" name="TopicName" column="[TopicName]" />
    <bag name="FkSlidesTopics" inverse="true" lazy="true" cascade="all">
      <key column="TopicID" />
      <one-to-many class="Slides" />
    </bag>
  </class>
</hibernate-mapping>