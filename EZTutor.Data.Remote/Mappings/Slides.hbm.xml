<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="EZTutor.Data.Remote" namespace="EZTutor.Data.Remote.Entities" >
  <class name="Slides" table="Slides" lazy="true">
    <id name="Slideid" column="SlideID" type="Guid">
      <generator class="assigned" />
    </id>
    <property type="string" not-null="true" name="SlideName" column="[SlideName]" />
    <many-to-one name="Lectureid" cascade="none" column="LectureID" not-null="true" />
    <many-to-one name="Topicid" cascade="none" column="TopicID" not-null="true" />
  </class>
</hibernate-mapping>