<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping namespace="EZTutor.Data.Remote" assembly="EZTutor.Data.Remote" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Lectures" table="Lectures" lazy="true">
    <id name="Lectureid" column="LectureID" type="Unknown">
      <generator class="assigned" />
    </id>
    <property type="Unknown" not-null="true" name="LectureName" column="[LectureName]" />
    <property type="Unknown" not-null="true" name="LectureWeek" column="[LectureWeek]" />
    <bag name="FkSlidesLectures" inverse="true" lazy="true" cascade="all">
      <key column="LectureID" />
      <one-to-many class="EZTutor.Data.Remote.Slides,EZTutor.Data.Remote" />
    </bag>
  </class>
  <class name="Slides" table="Slides" lazy="true">
    <id name="Slideid" column="SlideID" type="Unknown">
      <generator class="assigned" />
    </id>
    <property type="Unknown" not-null="true" name="SlideName" column="[SlideName]" />
    <many-to-one name="Lectureid" cascade="none" column="LectureID" not-null="true" />
    <many-to-one name="Topicid" cascade="none" column="TopicID" not-null="true" />
  </class>
  <class name="Topics" table="Topics" lazy="true">
    <id name="Topicid" column="TopicID" type="Unknown">
      <generator class="assigned" />
    </id>
    <property type="Unknown" not-null="true" name="TopicName" column="[TopicName]" />
    <bag name="FkSlidesTopics" inverse="true" lazy="true" cascade="all">
      <key column="TopicID" />
      <one-to-many class="EZTutor.Data.Remote.Slides,EZTutor.Data.Remote" />
    </bag>
  </class>
</hibernate-mapping>